{
  "skill-level": {
    "type": "union",
    "values": ["very_high", "pro", "hero"]
  },
  "player": {
    "type": "object",
    "attributes": {
      "steamId": { "type": "string", "optional": true },
      "heroName": { "type": "string" },
      "heroPortraitUrl": { "type": "string" }
    }
  },
  "comment": {
    "type": "object",
    "attributes": {
      "text": { "type": "string" },
      "timestamp": { "type": "number" }
    }
  },
  "metadata": {
    "type": "object",
    "attributes": {
      "mmr": { "type": "number" },
      "isParty": { "type": "boolean" },
      "isDisconnected": { "type": "boolean" }    
    }  
  }
}
